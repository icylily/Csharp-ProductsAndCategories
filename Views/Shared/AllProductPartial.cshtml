@model NewProductForm

<h3 class="text-center">Existed Products!</h3>
<hr>

<div  style="overflow:scroll; height:400px; border-left:solid 1px grey; padding-left:10px;">
    @foreach (var product in Model.CurrentProducts)
        {
            <li><a href="/products/@product.ProductId">@product.Name</a></li>
        }

</div>